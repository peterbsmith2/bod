{
  "version": 1,
  "projects": {
    "programming-kitchen": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/programming-kitchen",
      "sourceRoot": "apps/programming-kitchen/src",
      "prefix": "bod",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/programming-kitchen",
            "index": "apps/programming-kitchen/src/index.html",
            "main": "apps/programming-kitchen/src/main.ts",
            "polyfills": "apps/programming-kitchen/src/polyfills.ts",
            "tsConfig": "apps/programming-kitchen/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/programming-kitchen/src/favicon.ico",
              "apps/programming-kitchen/src/assets"
            ],
            "styles": ["apps/programming-kitchen/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/programming-kitchen/src/assets/scss"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/programming-kitchen/src/environments/environment.ts",
                  "with": "apps/programming-kitchen/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "programming-kitchen:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "programming-kitchen:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "programming-kitchen:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/programming-kitchen/tsconfig.app.json",
              "apps/programming-kitchen/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/programming-kitchen/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/programming-kitchen/jest.config.js",
            "tsConfig": "apps/programming-kitchen/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/programming-kitchen/src/test-setup.ts"
          }
        }
      }
    },
    "programming-kitchen-e2e": {
      "root": "apps/programming-kitchen-e2e",
      "sourceRoot": "apps/programming-kitchen-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/programming-kitchen-e2e/cypress.json",
            "tsConfig": "apps/programming-kitchen-e2e/tsconfig.e2e.json",
            "devServerTarget": "programming-kitchen:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "programming-kitchen:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/programming-kitchen-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/programming-kitchen-e2e/**"
            ]
          }
        }
      }
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/models/tsconfig.lib.json",
              "libs/models/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/models/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "tsConfig": "libs/models/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "programming-kitchen"
}
