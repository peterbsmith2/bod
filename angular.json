  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "none",
      "style": "scss"
    },
    "@nrwl/angular:ngrx": {
      "facade": false,
      "root": false
    },
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true
    }
  },
  "defaultProject": "trainer"
