        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/trainer/tsconfig.app.json",
              "apps/trainer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/trainer/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/trainer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-environments": {
